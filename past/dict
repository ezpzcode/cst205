color_dictionary = {'red': (255,0,0), 'green': (0,128,0), 'blue': (0,0,255), 'magenta': (255,0, 255), 'cyan': (0,255,255), 'yellow': (255,255,0)}

print("Blue value of magenta is", color_dictionary['magenta'][2])
print("Green value of yellow is", color_dictionary['yellow'][1])
print("Red value of cyan is", color_dictionary['cyan'][0])

for color in color_dictionary:
	if color[1] == "e":
		print(color, color_dictionary[color])